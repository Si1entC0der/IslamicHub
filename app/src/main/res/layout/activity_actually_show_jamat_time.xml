        android:gravity="center"
        android:text="Kalikapur Jame Mosjid"
        android:textColor="#FFEB3B"
        android:textSize="25sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="410dp"
        android:layout_height="463dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.735">

        <!--Sunrise time layout-->

        <!--Dhur time layout-->

        <RelativeLayout
            android:id="@+id/relativeLayout_fajr"
            android:layout_width="413dp"
            android:layout_height="45dp"
            android:layout_marginTop="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/fajrhead"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:fontFamily="@font/lato_regular"
                android:gravity="center|start"
                android:text="Fajr"
                android:textColor="@color/home_plate"
                android:textSize="34sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/get_fajr"
                android:layout_width="167dp"
                android:layout_height="46dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="35dp"
                android:layout_marginBottom="-2dp"
                android:fontFamily="@font/lato_regular"
                android:gravity="center|left"
                android:text="00:00"
                android:textColor="@color/home_plate"
                android:textSize="30sp"
                android:textStyle="normal" />


        </RelativeLayout>

        <!--    asar time layout-->
        <RelativeLayout
            android:id="@+id/relativeLayout_duhr"
            android:layout_width="410dp"
            android:layout_height="45dp"

            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout_fajr">

            <TextView
                android:id="@+id/duhrhead"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="-1dp"
                android:layout_marginBottom="1dp"
                android:fontFamily="@font/lato_regular"
                android:gravity="center|start"
                android:text="Duhr"
                android:textColor="#FFEB3B"
                android:textSize="34sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/get_dhur"
                android:layout_width="167dp"
                android:layout_height="46dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="30dp"
                android:fontFamily="@font/lato_regular"
                android:gravity="center|start"
                android:text="00:00"
                android:textColor="#CDDC39"
                android:textSize="30sp" />


        </RelativeLayout>

        <!--  sunset time layout  -->

        <!--Maghrib time layout-->
        <RelativeLayout
            android:id="@+id/relativeLayout_asar"
            android:layout_width="410dp"
            android:layout_height="45dp"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="parent"

            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout_duhr">

            <TextView
                android:id="@+id/asarhead"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="0dp"
                android:fontFamily="@font/lato_regular"
                android:gravity="center|start"
                android:text="Asar"
                android:textColor="#03A9F4"
                android:textSize="34sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/get_asar"
                android:layout_width="167dp"
                android:layout_height="46dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="32dp"
                android:fontFamily="@font/lato_regular"
                android:gravity="center|left"
                android:text="00:00"
                android:textColor="#03A9F4"
                android:textSize="30sp" />


        </RelativeLayout>

        <!--   Isha time layout-->


        <RelativeLayout
            android:id="@+id/relativeLayout_maghrib"
